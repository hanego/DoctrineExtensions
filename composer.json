{
    "name": "hanego/doctrine-extensions",
    "type": "library",
    "description": "Doctrine2 behavioral extensions for logging user, not username",
    "keywords": [
        "loggable"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lionel Gabaude",
            "email": ""
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "behat/transliterator": "~1.0",
        "doctrine/common": "~2.4",
        "gedmo/doctrine-extensions": "~2.4"
    },
    "require-dev": {
        "doctrine/mongodb-odm": "dev-master",
        "doctrine/orm": ">=2.5.0",
        "doctrine/common": ">=2.5.0",
        "symfony/yaml": "~2.6",
        "phpunit/phpunit": "~4.4",
        "phpunit/phpunit-mock-objects": "~2.3"
    },
    "suggest": {
        "doctrine/mongodb-odm": "to use the extensions with the MongoDB ODM",
        "doctrine/orm": "to use the extensions with the ORM"
    },
    "autoload": {
        "psr-0": { "Hanego\\": "lib/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
